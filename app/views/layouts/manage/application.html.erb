<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag 'manage/application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'manage/application', 'data-turbolinks-track': 'reload' %>
</head>
<body>

<% content_for :content do %>
  <%= render 'layouts/manage/header' %>

  <div class="container">
    <%= yield %>
  </div>

  <%= render 'layouts/manage/footer' %>
<% end %>
<%= render template: "layouts/application" %>

<div class="container">
  <%= debug(params) if Rails.env.development? %>
</div>
</body>
</html>